FormatVersion: 2.0.0
Version: 1.0.0
Name: Deploy Telnet

Parameters:
  appName: $appName

Body: |
  return telnetDeploy(args.appName).stdout

Scripts:
  telnetDeploy:
    Type: Application
    Version: 1.0.0
    EntryPoint: TelnetDeploy.sh
    Options:
      captureStdout: true
      captureStderr: false

